<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="td_radius" format="dimension" />
    <attr name="td_stroke" format="dimension" />
    <attr name="td_solid_color" format="reference|color" />
    <attr name="td_stroke_color" format="reference|color" />
    <attr name="td_gradient_startColor" format="reference|color" />
    <attr name="td_gradient_centerColor" format="reference|color" />
    <attr name="td_gradient_endColor" format="reference|color" />
    <attr name="td_gradient_orientation" format="integer">
        <enum name="top_bottom" value="0" />
        <enum name="tr_bl" value="1" />
        <enum name="right_left" value="2" />
        <enum name="br_tl" value="3" />
        <enum name="bottom_top" value="4" />
        <enum name="bl_tr" value="5" />
        <enum name="left_right" value="6" />
        <enum name="tl_br" value="7" />
    </attr>
    <attr name="td_shape">
        <enum name="rect" value="0" />
        <enum name="oval" value="1" />
    </attr>
    <attr name="td_topLeftRadius" format="dimension" />
    <attr name="td_topRightRadius" format="dimension" />
    <attr name="td_bottomRightRadius" format="dimension" />
    <attr name="td_bottomLeftRadius" format="dimension" />
    <attr name="td_dashWidth" format="dimension" />
    <attr name="td_dashGap" format="dimension" />
    <attr name="td_rippleColor" format="reference|color" />
    <attr name="td_background" format="reference|color" />
