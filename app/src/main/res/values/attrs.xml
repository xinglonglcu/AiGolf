        <attr name="td_bottomRightRadius" />
        <attr name="td_bottomLeftRadius" />
        <attr name="td_dashWidth" />
        <attr name="td_dashGap" />
        <attr name="tv_bold" format="boolean" />
        <attr name="td_rippleColor" />
        <attr name="td_background" />
    </declare-styleable>

